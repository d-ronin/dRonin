<xml>
  <object name="VTXSettings" settings="true" singleinstance="true">
    <description>Video transmitter settings.</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="true" updatemode="onchange" period="0"/>
    <telemetryflight acked="true" updatemode="onchange" period="0"/>
    <field defaultvalue="Active" elements="1" name="Mode" type="enum" units="">
      <description>Passive just monitors the VTx state while active modifies settings</description>
      <options>
        <option>Active</option>
        <option>Passive</option>
      </options>
    </field>
    <field defaultvalue="25" elements="1" name="Power" type="enum" units="mW">
      <description/>
      <options>
        <option>25</option>
        <option>200</option>
        <option>500</option>
        <option>800</option>
      </options>
    </field>
    <field defaultvalue="Band 5G8 A" elements="1" name="Band" type="enum" units="">
