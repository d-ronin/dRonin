      <options>
        <option>Band 5G8 A</option>
        <option>Band 5G8 B</option>
        <option>Band 5G8 E</option>
        <option>Airwave</option>
        <option>Raceband</option>
      </options>
    </field>
    <field defaultvalue="CH 1 5865" elements="1" name="Band_5G8_A_Frequency" type="enum" units="">
