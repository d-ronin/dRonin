        <option>CH 3 5825</option>
        <option>CH 4 5805</option>
        <option>CH 5 5785</option>
        <option>CH 6 5765</option>
        <option>CH 7 5745</option>
        <option>CH 8 5725</option>
      </options>
    </field>
    <field defaultvalue="CH 1 5733" elements="1" name="Band_5G8_B_Frequency" type="enum" units="">
