<xml>
  <object name="HwAQ32" settings="true" singleinstance="true">
    <description>Selection of optional hardware configurations.</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="true" updatemode="onchange" period="0"/>
    <telemetryflight acked="true" updatemode="onchange" period="0"/>
    <field defaultvalue="Disabled" elements="1" name="RcvrPort" parent="HwShared.PortTypes" type="enum" units="function">
      <description>The pin labelled Servo3</description>
      <options>
        <option>Disabled</option>
        <option>PPM</option>
      </options>
    </field>
    <field defaultvalue="Disabled" elements="1" name="Uart1" parent="HwShared.PortTypes" type="enum" units="function">
      <description>The port labelled "SER"</description>
      <options>
        <option>Disabled</option>
        <option>ComBridge</option>
        <option>DebugConsole</option>
        <option>FrSKY Sensor Hub</option>
        <option>FrSKY SPort Telemetry</option>
        <option>FrSKY SPort Non Inverted</option>
        <option>HoTT Telemetry</option>
        <option>LighttelemetryTx</option>
        <option>MavLinkTX</option>
        <option>MSP</option>
        <option>OpenLog</option>
        <option>TBS Crossfire</option>
        <option>Telemetry</option>
      </options>
    </field>
    <field defaultvalue="Disabled" elements="1" name="Uart2" parent="HwShared.PortTypes" type="enum" units="function">
      <description>The port labelled "GPS"</description>
      <options>
        <option>Disabled</option>
        <option>ComBridge</option>
        <option>GPS</option>
        <option>MavLinkTX_GPS_RX</option>
      </options>
    </field>
    <field defaultvalue="Disabled" elements="1" name="Uart3" parent="HwShared.PortTypes" type="enum" units="function">
      <description>The port labelled "SBUS"</description>
      <options>
        <option>Disabled</option>
        <option>ComBridge</option>
        <option>DebugConsole</option>
        <option>FrSKY Sensor Hub</option>
        <option>FrSKY SPort Telemetry</option>
        <option>FrSKY SPort Non Inverted</option>
        <option>GPS</option>
        <option>SRXL</option>
        <option>HoTT SUMD</option>
        <option>HoTT SUMH</option>
        <option>HoTT Telemetry</option>
        <option>LighttelemetryTx</option>
        <option>MavLinkTX</option>
        <option>MavLinkTX_GPS_RX</option>
        <option>MSP</option>
        <option>OpenLog</option>
        <option>S.Bus</option>
        <option>S.Bus Non Inverted</option>
        <option>Telemetry</option>
        <option>IBus</option>
        <option>TBS Crossfire</option>
      </options>
    </field>
    <field defaultvalue="Disabled" elements="1" name="Uart4" parent="HwShared.PortTypes" type="enum" units="function">
      <description>The "TX" pin at "Camera 1" and "RX" pin at "Camera 2"</description>
      <options>
        <option>Disabled</option>
        <option>ComBridge</option>
        <option>DebugConsole</option>
        <option>DSM</option>
        <option>FrSKY Sensor Hub</option>
        <option>FrSKY SPort Telemetry</option>
        <option>FrSKY SPort Non Inverted</option>
        <option>GPS</option>
        <option>SRXL</option>
        <option>HoTT SUMD</option>
        <option>HoTT SUMH</option>
        <option>HoTT Telemetry</option>
        <option>LighttelemetryTx</option>
        <option>MavLinkTX</option>
        <option>MavLinkTX_GPS_RX</option>
        <option>MSP</option>
        <option>OpenLog</option>
        <option>S.Bus Non Inverted</option>
        <option>Telemetry</option>
        <option>IBus</option>
        <option>TBS Crossfire</option>
      </options>
    </field>
    <field defaultvalue="Disabled" elements="1" name="Uart6" parent="HwShared.PortTypes" type="enum" units="function">
      <description>The "RX" pin at "Motor 3" and "TX" pin at "Motor 4"</description>
      <options>
        <option>Disabled</option>
        <option>ComBridge</option>
        <option>DebugConsole</option>
        <option>DSM</option>
        <option>FrSKY Sensor Hub</option>
        <option>FrSKY SPort Telemetry</option>
        <option>FrSKY SPort Non Inverted</option>
        <option>GPS</option>
        <option>SRXL</option>
        <option>HoTT SUMD</option>
        <option>HoTT SUMH</option>
        <option>HoTT Telemetry</option>
        <option>LighttelemetryTx</option>
        <option>MavLinkTX</option>
        <option>MavLinkTX_GPS_RX</option>
        <option>MSP</option>
        <option>OpenLog</option>
        <option>S.Bus Non Inverted</option>
        <option>Telemetry</option>
        <option>IBus</option>
        <option>TBS Crossfire</option>
      </options>
    </field>
    <field defaultvalue="Disabled" elements="1" name="ADCInputs" type="enum" units="function">
      <description>Analog-to-digital inputs</description>
      <options>
        <option>Disabled</option>
        <option>Enabled</option>
      </options>
    </field>
    <field defaultvalue="Disabled" elements="1" name="OSDPort" type="enum" units="function">
      <description/>
      <options>
        <option>Disabled</option>
        <option>MAX7456 OSD</option>
      </options>
    </field>
    <field defaultvalue="USBTelemetry" elements="1" name="USB_HIDPort" type="enum" units="function">
      <description/>
      <options>
        <option>USBTelemetry</option>
        <option>Disabled</option>
      </options>
    </field>
    <field defaultvalue="Disabled" elements="1" name="USB_VCPPort" parent="HwShared.USB_VCPPort" type="enum" units="function">
      <description>Virtual COM Port protocol</description>
    </field>
    <field defaultvalue="Autodetect" elements="1" name="DSMxMode" parent="HwShared.DSMxMode" type="enum" units="mode">
      <description/>
    </field>
    <field defaultvalue="1000" elements="1" name="GyroRange" type="enum" units="deg/s">
      <description>Maximum measurement range for the gyros, should never rotate faster than about 85% of this</description>
      <options>
        <option>250</option>
        <option>500</option>
        <option>1000</option>
        <option>2000</option>
      </options>
    </field>
    <field defaultvalue="8G" elements="1" name="AccelRange" type="enum" units="*gravity m/s^2">
      <description>The maximum measure range for the accelerometers, should never exceed this</description>
      <options>
        <option>2G</option>
        <option>4G</option>
        <option>8G</option>
        <option>16G</option>
      </options>
    </field>
    <field defaultvalue="188" elements="1" name="MPU6000DLPF" type="enum" units="">
      <description>Gyro filter cutoff frequency</description>
      <options>
        <option>188</option>
        <option>98</option>
        <option>42</option>
        <option>20</option>
        <option>10</option>
        <option>5</option>
      </options>
    </field>
    <field defaultvalue="Internal" elements="1" name="Magnetometer" parent="HwShared.Mag" type="enum" units="">
      <description>Which magnetometer to use.</description>
    </field>
    <field defaultvalue="Top0degCW" elements="1" name="ExtMagOrientation" parent="HwShared.MagOrientation" type="enum" units="">
      <description>Orientation of the external magnetometer</description>
    </field>
  </object>
</xml>
