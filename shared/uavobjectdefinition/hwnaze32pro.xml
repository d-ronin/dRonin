<xml>
	<object name="HwNaze32Pro" settings="true" singleinstance="true">
		<description>Selection of optional hardware configurations.</description>
		<access gcs="readwrite" flight="readwrite"/>
		<logging updatemode="manual" period="0"/>
		<telemetrygcs acked="true" updatemode="onchange" period="0"/>
		<telemetryflight acked="true" updatemode="onchange" period="0"/>
		<field defaultvalue="Disabled" elements="1" name="RcvrPort" parent="HwShared.PortTypes" type="enum" units="function">
			<options>
				<option>Disabled</option>
				<option>DSM</option>
				<option>HoTT SUMD</option>
				<option>HoTT SUMH</option>
				<option>PPM</option>
				<option>S.Bus</option>
				<option>S.Bus Non Inverted</option>
				<option>TBS Crossfire</option>
			</options>
		</field>
		<field defaultvalue="Disabled" elements="1" name="Uart1" parent="HwShared.PortTypes" type="enum" units="function">
			<options>
				<option>Disabled</option>
				<option>ComBridge</option>
				<option>DebugConsole</option>
				<option>DSM</option>
				<option>FrSKY Sensor Hub</option>
				<option>FrSKY SPort Telemetry</option>
				<option>GPS</option>
				<option>HoTT Telemetry</option>
				<option>LighttelemetryTx</option>
				<option>MavLinkTX</option>
				<option>MavLinkTX_GPS_RX</option>
				<option>MSP</option>
				<option>OpenLog</option>
				<option>Storm32Bgc</option>
				<option>TBS Crossfire</option>
				<option>Telemetry</option>
			</options>
		</field>
		<field defaultvalue="Disabled" elements="1" name="Uart2" parent="HwShared.PortTypes" type="enum" units="function">
			<options>
				<option>Disabled</option>
				<option>ComBridge</option>
				<option>DebugConsole</option>
				<option>DSM</option>
				<option>FrSKY Sensor Hub</option>
				<option>FrSKY SPort Telemetry</option>
				<option>GPS</option>
				<option>HoTT Telemetry</option>
				<option>LighttelemetryTx</option>
				<option>MavLinkTX</option>
				<option>MavLinkTX_GPS_RX</option>
				<option>MSP</option>
				<option>OpenLog</option>
				<option>Storm32Bgc</option>
				<option>TBS Crossfire</option>
				<option>Telemetry</option>
			</options>
		</field>
		<field defaultvalue="PWM8" elements="1" name="OutPort" type="enum" units="function">
			<options>
				<option>PWM8</option>
				<option>PWM8+ADC1</option>
				<option>PWM8+ADC2</option>
				<option>PWM8+ADC3</option>
			</options>
		</field>
		<field defaultvalue="USBTelemetry" elements="1" name="USB_HIDPort" parent="HwShared.USB_HIDPort" type="enum" units="function">
			<description/>
		</field>
		<field defaultvalue="Disabled" elements="1" name="USB_VCPPort" parent="HwShared.USB_VCPPort" type="enum" units="function">
			<description>Virtual COM Port protocol</description>
		</field>
		<field defaultvalue="Autodetect" elements="1" name="DSMxMode" parent="HwShared.DSMxMode" type="enum" units="mode">
			<description/>
		</field>
		<field defaultvalue="2000" elements="1" name="GyroRange" type="enum" units="deg/s">
			<description/>
			<options>
				<option>250</option>
				<option>500</option>
				<option>1000</option>
				<option>2000</option>
			</options>
		</field>
		<field defaultvalue="8G" elements="1" name="AccelRange" type="enum" units="*gravity m/s^2">
			<description/>
			<options>
				<option>2G</option>
				<option>4G</option>
				<option>8G</option>
				<option>16G</option>
			</options>
		</field>
		<field defaultvalue="188" elements="1" name="MPU6000DLPF"  type="enum" units="Hz">
			<description/>
			<options>
				<option>188</option>
				<option>98</option>
				<option>42</option>
				<option>20</option>
				<option>10</option>
				<option>5</option>
			</options>
		</field>
		<field defaultvalue="Internal" elements="1" name="Magnetometer" parent="HwShared.Mag" type="enum" units="function">
			<options>
				<option>Internal</option>
				<option>External HMC5883</option>
				<option>External HMC5983</option>
			</options>
		</field>
		<field defaultvalue="Top0degCW" elements="1" name="ExtMagOrientation" parent="HwShared.MagOrientation" type="enum" units="function">
			<description/>
		</field>
    </object>
