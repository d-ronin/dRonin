<xml>
    <object name="TriflightStatus" singleinstance="true" settings="false">
        <description>Triflight Status</description>
        <field name="Initialized" units="" type="enum" elements="1" options="False,True"/>
        <field name="RearMotorChannel" units="" type="uint8" elements="1"/>
        <field name="ServoChannel" units="" type="uint8" elements="1"/>
        <field name="UncorrectedServoCmd" units="" type="float" elements="1"/>
        <field name="CorrectedServoCmd" units="" type="float" elements="1"/>
        <field name="ADCServoFdbk" units="" type="float" elements="1"/>
        <field name="ServoAngle" units="" type="float" elements="1"/>
        <field name="MotorCorrection" units="" type="float" elements="1"/>
        <field name="VirtualTailMotor" units="" type="float" elements="1"/>
        <field name="DynamicYawGain" units="" type="float" elements="1"/>
        <field name="ExpectedGyroError" units="" type="float" elements="1"/>

        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="false" updatemode="manual" period="0"/>
        <telemetryflight acked="false" updatemode="throttled" period="1000"/>
        <logging updatemode="manual" period="0"/>
    </object>
</xml>
