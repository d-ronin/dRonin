			<options>
				 <option>False</option>
				 <option>True</option>
			</options>
		</field>

		<field elements="2" name="MotorChannel" type="uint8" units=""/>
		
		<field elements="2" name="ServoChannel" type="uint8" units=""/>
		
		<field elements="2" name="ADCServoFdbk" type="float" units=""/>

		<field elements="2" name="ServoAngle" type="float" units=""/>
	</object>
</xml>
