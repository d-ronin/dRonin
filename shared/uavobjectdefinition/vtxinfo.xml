<xml>
  <object name="VTXInfo" settings="false" singleinstance="true">
    <description>Video transmitter information.</description>
    <access gcs="readwrite" flight="readwrite"/>
    <logging updatemode="manual" period="0"/>
    <telemetrygcs acked="true" updatemode="onchange" period="0"/>
    <telemetryflight acked="true" updatemode="onchange" period="0"/>
    <field defaultvalue="None" elements="1" name="Model" type="enum" units="">
      <description/>
      <options>
        <option>None</option>
        <option>TBS Unify Pro 5G8</option>
        <option>TBS Unify Pro 5G8 HV</option>
        <option>TBS Unify Evo 5G8 HV</option>
      </options>
    </field>
    <field defaultvalue="0" elements="1" name="Frequency" type="uint16" units="MHz">
      <description/>
    </field>
    <field defaultvalue="0" elements="1" name="Power" type="uint16" units="mW">
      <description/>
    </field>
    <field defaultvalue="Disconnected" elements="1" name="State" type="enum" units="">
      <description/>
      <options>
        <option>Disconnected</option>
        <option>Connected</option>
      </options>
    </field>
  </object>
</xml>
