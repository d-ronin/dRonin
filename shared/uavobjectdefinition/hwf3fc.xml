<xml>
	<object name="HwF3Fc" settings="true" singleinstance="true">
		<description>Selection of optional hardware configurations.</description>
		<access gcs="readwrite" flight="readwrite"/>
		<logging updatemode="manual" period="0"/>
		<telemetrygcs acked="true" updatemode="onchange" period="0"/>
		<telemetryflight acked="true" updatemode="onchange" period="0"/>
		<field defaultvalue="Disabled" elements="1" name="PPM_Receiver" type="enum" units="function">
			<options>
				<option>Disabled</option>
				<option>Enabled</option>
			</options>
		</field>
		<field defaultvalue="Disabled" elements="1" name="Uart1" parent="HwShared.PortTypes" type="enum" units="function">
			<options>
				<option>Disabled</option>
				<option>ComBridge</option>
				<option>DebugConsole</option>
				<option>DSM</option>
				<option>FrSKY Sensor Hub</option>
				<option>FrSKY SPort Telemetry</option>
				<option>GPS</option>
				<option>HoTT Telemetry</option>
				<option>IBus</option>
				<option>MSP</option>
				<option>OpenLog</option>
				<option>S.Bus</option>
				<option>S.Bus Non Inverted</option>
				<option>SRXL</option>
				<option>TBS Crossfire</option>
				<option>Telemetry</option>
			</options>
		</field>
		<field defaultvalue="Disabled" elements="1" name="Uart2" parent="HwShared.PortTypes" type="enum" units="function">
			<options>
				<option>Disabled</option>
				<option>ComBridge</option>
				<option>DebugConsole</option>
				<option>DSM</option>
				<option>FrSKY Sensor Hub</option>
				<option>FrSKY SPort Telemetry</option>
				<option>GPS</option>
				<option>HoTT Telemetry</option>
				<option>IBus</option>
				<option>MSP</option>
				<option>OpenLog</option>
				<option>S.Bus</option>
				<option>S.Bus Non Inverted</option>
				<option>SRXL</option>
				<option>TBS Crossfire</option>
				<option>Telemetry</option>
			</options>
		</field>
		<field defaultvalue="Disabled" elements="1" name="Uart3" parent="HwShared.PortTypes" type="enum" units="function">
			<options>
				<option>Disabled</option>
				<option>ComBridge</option>
				<option>DebugConsole</option>
				<option>DSM</option>
				<option>FrSKY Sensor Hub</option>
				<option>FrSKY SPort Telemetry</option>
				<option>GPS</option>
				<option>HoTT Telemetry</option>
				<option>IBus</option>
				<option>MSP</option>
				<option>OpenLog</option>
				<option>S.Bus</option>
				<option>S.Bus Non Inverted</option>
				<option>SRXL</option>
				<option>TBS Crossfire</option>
				<option>Telemetry</option>
			</options>
		</field>
		<field defaultvalue="USBTelemetry" elements="1" name="USB_HIDPort" parent="HwShared.USB_HIDPort" type="enum" units="function">
			<description/>
		</field>
		<field defaultvalue="Disabled" elements="1" name="USB_VCPPort" parent="HwShared.USB_VCPPort" type="enum" units="function">
			<description>Virtual COM Port protocol</description>
		</field>
		<field defaultvalue="Autodetect" elements="1" name="DSMxMode" parent="HwShared.DSMxMode" type="enum" units="mode">
			<description/>
		</field>
		<field defaultvalue="2000" elements="1" name="GyroRange" type="enum" units="deg/s">
			<description>Maximum measurement range for the gyros, should never rotate faster than about 85% of this</description>
			<options>
				<option>250</option>
				<option>500</option>
				<option>1000</option>
				<option>2000</option>
			</options>
		</field>
		<field defaultvalue="8G" elements="1" name="AccelRange" type="enum" units="*gravity m/s^2">
			<description>The maximum measure range for the accelerometers, should never exceed this</description>
			<options>
				<option>2G</option>
				<option>4G</option>
				<option>8G</option>
				<option>16G</option>
			</options>
		</field>
		<field defaultvalue="188" elements="1" name="MPU6000DLPF" type="enum" units="Hz">
			<description>Gyro filter cutoff frequency</description>
			<options>
				<option>188</option>
				<option>98</option>
				<option>42</option>
				<option>20</option>
				<option>10</option>
				<option>5</option>
			</options>
		</field>
		<field defaultvalue="None" elements="1" name="MS5611" type="enum" units="none">
			<description>Enable/Disable MS5611</description>
			<options>
				<option>None</option>
				<option>Enabled</option>
			</options>
		</field>
		<field defaultvalue="None" elements="1" name="ExtMag" parent="HwShared.Mag" type="enum" units="none">
			<description>Enable/Disable External Mag</description>
			<options>
				<option>None</option>
				<option>External HMC5883</option>
				<option>External HMC5983</option>
			</options>
		</field>
		<field defaultvalue="Top0degCW" elements="1" name="ExtMagOrientation" parent="HwShared.MagOrientation" type="enum" units="none">
			<description>Orientation of the external magnetometer</description>
		</field>
		<field defaultvalue="Uart2_RX" elements="1" name="FC_Pad_R2" type="enum" units="none">
			<description>R2 Pad Function</description>
			<options>
				<option>Fdbk2</option>
				<option>Uart2_RX</option>
			</options>
		</field>
	</object>
</xml>
